{
  "name": "thronesking",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:fix": "yarn lint:js --fix && yarn lint:style --fix",
    "lint": "yarn lint:js && yarn lint:style"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "*.{css,vue}": "stylelint **/*.{vue,css} --ignore-path .gitignore --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0",
    "@nuxtjs/axios": "^5.13",
    "@nuxtjs/pwa": "^3.3",
    "@nuxtjs/strapi": "^0.1",
    "bulma": "^0.9.2",
    "core-js": "^3.8",
    "lodash-es": "^4.17.20",
    "nuxt": "^2.15",
    "postcss-color-mod-function": "^3.0",
    "postcss-comment": "^2.0",
    "postcss-custom-properties": "^9.1",
    "postcss-nested": "^4.2"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15",
    "@nuxt/typescript-build": "^2.0",
    "@nuxtjs/eslint-config-typescript": "^5.0",
    "@nuxtjs/eslint-module": "^3.0",
    "@nuxtjs/stylelint-module": "^4.0",
    "babel-eslint": "^10.1",
    "eslint": "^7.20",
    "eslint-config-prettier": "^7.2",
    "eslint-plugin-nuxt": "^2.0",
    "eslint-plugin-prettier": "^3.3",
    "eslint-plugin-vue": "^7.6",
    "husky": "^4.3",
    "lint-staged": "^10.5",
    "prettier": "^2.2",
    "stylelint": "^13.10",
    "stylelint-config-prettier": "^8.0",
    "stylelint-config-standard": "^20.0"
  }
}
